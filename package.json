{
  "name": "root",
  "private": true,
  "workspaces": [
    "packages/*",
    "constants/*"
  ],
  "scripts": {
    "bootstrap": "lerna bootstrap",
    "prewatch": "npm run bootstrap",
    "upint": "yarn upgrade-interactive",
    "pub": "lerna publish --force-publish",
    "build": "lerna exec --parallel -- rollup -c ../../rollup.config.mjs",
    "build-specific": "lerna exec --scope=@texting/enum-brackets -- rollup -c ../../rollup.config.mjs",
    "watch": "lerna exec --parallel -- rollup -c ../../rollup.config.mjs -w",
    "watch-specific": "lerna exec --scope=@texting/art-project -- rollup -c ../../rollup.config.mjs -w"
  },
  "devDependencies": {
    "@analys/crostab": "^0.6.14",
    "@babel/core": "^7.18.2",
    "@babel/plugin-external-helpers": "^7.17.12",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-decorators": "^7.18.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.17.12",
    "@babel/plugin-proposal-optional-chaining": "^7.17.12",
    "@babel/plugin-proposal-pipeline-operator": "^7.18.2",
    "@babel/plugin-proposal-private-methods": "^7.17.12",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.18.3",
    "@foba/util": "^0.5.7",
    "@palett/says": "^0.9.10",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@spare/deco": "^0.14.9",
    "@spare/logger": "^0.14.9",
    "@valjoux/strategies": "^0.3.17",
    "@valjoux/timestamp-pretty": "^0.3.17",
    "@vect/vector-init": "^0.7.14",
    "lerna": "^5.1.1",
    "rollup": "^2.75.6",
    "rollup-plugin-fileinfo": "^0.3.4",
    "rollup-plugin-terser": "^7.0.2"
  },
  "packageManager": "yarn@3.2.0"
}
